{
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-console": "off",
        "linebreak-style": 0
    }
}